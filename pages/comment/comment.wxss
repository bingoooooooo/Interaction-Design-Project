.comment {
  margin: 0;
  padding: 16rpx 24rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 1rpx solid #cccccc;
  border-bottom: 1rpx solid #cccccc;
  background-color:#fcfcfc;
  box-sizing: border-box;
  font-family: PingFangSC-Regular, PingFang SC;
  font-size: 32rpx;
  transition: all 2s inherit;
  overflow: hidden;
  position: fixed;
  left: 0;
}
 
.textarea {
  margin: 0;
  padding: 11rpx 24rpx;
  width: calc(100% - 100rpx - 50rpx - 24rpx);
  min-height: 40.8rpx;
  line-height: 40.8rpx;
  outline: none;
  border: 1rpx solid #cccccc;
  border-radius: 15rpx;
  background-color: #FFFFFF;
  box-sizing: content-box;
  overflow: hidden;
}
 
.send_out {
  margin: 0;
  padding: 0;
  width: 100rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border: 1rpx solid #cccccc;
  border-radius: 10rpx;
  /* 将发送按钮固定在底部 */
  position: absolute;
  right: 24rpx;
}
.modals {
  position:fixed;
  z-index:2;
  top:0;
  left:0;
  right:0;
  bottom:0;
 }
  
 .modals-cancel {
  position:absolute;
  z-index:3;
  top:0;
  left:0;
  right:0;
  bottom:0;
  background-color: rgba(0,0,0, .5);
 }
  
